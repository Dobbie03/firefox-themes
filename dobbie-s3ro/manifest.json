{
  "manifest_version": 2,
  "version": "1.2",
  "name": "Dobbie s3ro",
  "author": "Matthew Dobson",
  "description": "Unified simple dark theme that removes borders colours from nixie-s3ro GTK theme",
  "homepage_url": "https://github.com/dobbie03/firefox-themes",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "theme": {
    "colors": {
      "bookmark_text": "#798c91",
      "button_background_active": "#1f1f1f",
      "button_background_hover": "#1f1f1f",
      "frame_inactive": "#212121",
      "frame": "#212121",
      "icons_attention": "#798c91",
      "icons": "#535c5c",
      "ntp_background": "#212121",
      "ntp_text": "#798c91",
      "popup_border": "#212121",
      "popup_highlight_text": "#798C91",
      "popup_highlight": "rgba(255, 255, 255, 0.1)",
      "popup_text": "#798c91",
      "popup": "#212121",
      "sidebar_border": "#212121",
      "sidebar_highlight_text": "#798c91",
      "sidebar_highlight": "#1f1f1f",
      "sidebar_text": "#798c91",
      "sidebar": "#212121",
      "tab_background_separator": "#212121",
      "tab_background_text": "#a9a9ab",
      "tab_line": "#535c5c",
      "tab_loading": "#798c91",
      "tab_selected": "#212121",
      "tab_text": "#798c91",
      "toolbar_bottom_separator": "#212121",
      "toolbar_field_border_focus": "#212121",
      "toolbar_field_border": "#212121",
      "toolbar_field_focus": "#1f1f1f",
      "toolbar_field_highlight_text": "#798C91",
      "toolbar_field_highlight": "#212121",
      "toolbar_field_separator": "#212121",
      "toolbar_field_text_focus": "#798c91",
      "toolbar_field_text": "#798c91",
      "toolbar_field": "#212121",
      "toolbar_top_separator": "r#1f1f1f",
      "toolbar_vertical_separator": "#212121",
      "toolbar": "#212121"
    }
